msgstr ""

#: lib/krb5.c:734
msgid "Message context invalid"
msgstr ""

#: lib/krb5.c:736
msgid "Buffer is the wrong size"
msgstr ""

#: lib/krb5.c:738
msgid "Credential usage type is unknown"
msgstr ""

#: lib/krb5.c:740
msgid "Unknown quality of protection specified"
msgstr ""

#: lib/krb5.c:743
msgid "Principal in credential cache does not match desired name"
msgstr ""

#: lib/krb5.c:745
msgid "No principal in keytab matches desired name"
msgstr ""

#: lib/krb5.c:747
msgid "Credential cache has no TGT"
msgstr ""

#: lib/krb5.c:749
msgid "Authenticator has no subkey"
msgstr ""

#: lib/krb5.c:751
msgid "Context is already fully established"
msgstr "Ingen säkerhetskontext har upprättats"

#: lib/krb5.c:753
msgid "Unknown signature type in token"
msgstr ""

#: lib/krb5.c:755
msgid "Invalid field length in token"
msgstr ""

#: lib/krb5.c:757
msgid "Attempt to use incomplete security context"
msgstr ""

#: lib/krb5.c:774
msgid "No krb5 error"
msgstr "Inget krb5 fel"

#: lib/krb5.c:802
msgid "Unknown krb5 error"
msgstr "Okänt krb5 fel"

#: lib/obsolete.c:30
msgid "warning: gss_sign() is obsolete, use gss_get_mic() instead."
msgstr "varning: gss_sign() är obsolet, använd gss_get_mic() istället."

#: lib/obsolete.c:43
msgid "warning: gss_verify() is obsolete, use gss_verify_mic() instead."
msgstr "varning: gss_verify() är obsolet, använd gss_verify_mic() istället."

#: lib/obsolete.c:57
msgid "warning: gss_seal() is obsolete, use gss_wrap() instead."
msgstr "varning: gss_seal() är obsolet, använd gss_wrap() istället."

#: lib/obsolete.c:71
msgid "warning: gss_unseal() is obsolete, use gss_unwrap() instead."
msgstr "varning: gss_unseal() är obsolet, använd gss_unwrap() istället."

#: lib/meta.c:31
msgid "warning: no suitable mechanism found\n"
msgstr "varning: ingen passande mekanism hittad\n"

#: lib/meta.c:43
msgid "No suitable mechanism supported"
msgstr "Ingen passande mekanism hittad"
